{
    "num_spaces_for_tab": 4,
    "show_margin": false,
    "continue_comments_on_newline": true,
    "editor_keybindings": "vim",
    "soft_wrap_r_files": true,
    "editor_theme": "One Dark {rsthemes}",
    "default_project_location": "~/Google Drive/personal/Learning/Courses/Statistics and R",
    "default_latex_program": "XeLaTeX",
    "wrap_tab_navigation": false,
    "save_workspace": "never",
    "reuse_sessions_for_project_links": true,
    "panes": {
        "quadrants": [
            "Source",
            "Console",
            "TabSet1",
            "TabSet2"
        ],
        "tabSet1": [
            "Environment",
            "History",
            "Connections",
            "Build",
            "VCS",
            "Tutorial",
            "Presentation"
        ],
        "tabSet2": [
            "Files",
            "Plots",
            "Packages",
            "Help",
            "Viewer",
            "Presentations"
        ],
        "console_left_on_top": false,
        "console_right_on_top": true
    },
    "posix_terminal_shell": "bash",
    "help_font_size_points": 12,
    "jobs_tab_visibility": "shown",
    "highlight_r_function_calls": true,
    "rainbow_parentheses": true,
    "initial_working_directory": "~",
    "global_theme": "alternate",
    "font_size_points": 11,
    "show_hidden_files": true,
    "knit_working_dir": "project"
}